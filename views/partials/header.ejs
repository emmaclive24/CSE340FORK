<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page" id="return">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (loggedin) { %>
            <span id="user_head">Welcome <%- accountData.account_firstname %> <a href="/account" title="Explore your Page"><%- accountData.account_type %></a> User |
            <a href="/account/logout" title="Account Log-Out">Logout</a></span>
        <% } else { %>
            <a href="/account/login" title="Click to log in">My Account</a>
        <% } %>
    </div>
</header>